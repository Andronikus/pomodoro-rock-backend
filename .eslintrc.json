{
    "env": {
        "node": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "prettier",
        "simple-import-sort"
    ],
    "rules": {
        "prettier/prettier": "error",
        "simple-import-sort/sort": "warn"
    }
}
